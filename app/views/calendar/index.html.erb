<h1>Calendars</h1>
<%= flash[:notice] %>
<table>
  <tr>
    <th>Summary</th>
    <th>Start Time</th>
    <th>Time Zone</th>
  </tr>
<% @calendars.each do |calendar| %>
  <tr>
    <td><%= calendar.summary %></td>
    <td><%= calendar.starttime %></td>
    <td><%= calendar.timezone %></td>
  </tr>
<% end %>
</table>

<%= form_tag('import', :multipart => true) do %>
	<p>
  <%= label_tag(:file, "Browse File:") %>
  <%= file_field_tag(:file) %>
 </p>
  <p><%= submit_tag("Import File") %></p>
  
<% end %>
<%= button_to 'Export File', 'export', method: :post %>

<%= link_to 'Back to Main page', '/' %>

