<h1>Calendars</h1>
<table>
  <tr>
    <th>Summary</th>
    <th>Start Time</th>
    <th>Time Zone</th>
  </tr>
<% @calendars.each do |calendar| %>
  <tr>
    <td><%= calendar.summary %></td>
    <td><%= calendar.starttime %></td>
    <td><%= calendar.timezone %></td>
  </tr>
<% end %>
</table>
<%= link_to 'Back to Main page', '/' %>
